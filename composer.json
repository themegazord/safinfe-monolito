{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^3.1",
        "commerceguys/intl": "^2.0",
        "geekcom/validator-docs": "^3.11",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.7.4",
        "phpoffice/phpspreadsheet": "^3.3",
        "robsontenorio/mary": "^2.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "spatie/laravel-backup": "^9.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/telescope": "^5.15",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": [
          "./vendor/bin/pint"
        ],
        "all": [
          "@post-autoload-dump",
          "@post-update-cmd",
          "@post-root-package-install",
          "@post-create-project-cmd"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev-wsl": [
          "Composer\\Config::disableProcessTimeout",
          "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74,#e5a78d\" \"php artisan serve --host=0.0.0.0 --port=8000\" \"php artisan queue:listen --tries=1\" \"php artisan schedule:work\" \"npm run dev -- --host=0.0.0.0 --port=5173\" --names=server,queue,schedule,vite"
        ],
        "dev": [
          "Composer\\Config::disableProcessTimeout",
          "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185\" \"php artisan serve --host=192.168.0.5\" \"php artisan queue:listen\" \"npm run dev -- --host=192.168.0.5\" --names=server,queue,vite"
        ],
        "devD": [
          "Composer\\Config::disableProcessTimeout",
          "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74,#e5a78d,#a3d8a1\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" \"d:/php/mailpit.exe\" \"php artisan reverb:start\" --names=server,queue,vite,mailpit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
